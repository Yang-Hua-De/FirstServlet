<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>DeptFindServlet</servlet-name>
        <servlet-class>com.bjpowernode.controller.DeptFindServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmpFindServlet</servlet-name>
        <servlet-class>com.bjpowernode.controller.EmpFindServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeptFindServlet</servlet-name>
        <url-pattern>/dept/find.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmpFindServlet</servlet-name>
        <url-pattern>/emp/find.do</url-pattern>
    </servlet-mapping>
</web-app>
